{
    "context_length": 168,
    "prediction_length": 24,
    "dataset_path": "./dataset_in",
    "batch_size": 64,

    "timestamp_column": "time",
    "id_columns": [],
    "forecast_columns": ["energy"],

    "patch_length": 24,
    "patch_stride": 24,
    "num_input_channels": 1, 
    "is_scaling": true,

    "d_model": 16,
    "num_layers": 3, 
    "expansion_factor": 3,
    "dropout": 0.3,
    "head_dropout": 0.2,
    "mode": ["common_channel", "mix_channel"],
    "gated_attn": true,
    "norm_mlp": "LayerNorm",
    "self_attn": false,
    "self_attn_heads": 1,
    "use_positional_encoding": false,
    "positional_encoding_type": "sincos",
    "scaling": "std",
    "loss": "mse",
    "init_std": 0.02,
    "post_init": false,
    "norm_eps": 1e-5,
    "adaptive_patching_levels": 3,
    "resolution_prefix_tuning": false,
    "frequency_token_vocab_size": 5,
    "distribution_output": "student_t",
    "num_parallel_samples": 100,

    "decoder_num_layers": 8,
    "decoder_d_model": 8,
    "decoder_adaptive_patching_levels": 0,
    "decoder_raw_residual": false,
    "decoder_mode": "common_channel",
    "use_decoder": true,

    "enable_forecast_channel_mixing": false,
    "fcm_gated_attn": true,
    "fcm_context_length": 1,
    "fcm_use_mixer": false,
    "fcm_mix_layers":2,
    "fcm_prepend_past": true,
    "init_linear": "pytorch",
    "init_embed": "pytorch",



    "output_dir": "./checkpoint/TTMs/output",
    "logging_dir": "./checkpoint/TTMs/logs",    
    "num_epochs": 100,
    "evaluation_strategy": "epoch",
    "save_strategy": "epoch",
    "save_total_limit": 5,
    "logging_strategy": "epoch",
    "load_best_model_at_end": true,
    "label_names": ["future_values"],

    "model_save_path": "./model_weights/TTMs",
    "config_file": "./model_weights/TTMs",
    "early_stopping_patience": 10,
    "early_stopping_threshold": 0.0001,
    "learning_rate": 0.001,

    "do_eval": true,
    "num_workers": 0,
    "report_to": "tensorboard",
    "greater_is_better": false,
    "metric_for_best_model": "eval_loss",
    "overwrite_output_dir": true

}